@model IEnumerable<MusicApp.Models.AlbumBaseViewModel>

@{
    ViewBag.Title = "Album List";
}

<h2>@ViewBag.Title</h2>
<hr />

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
        </tr>
    }

</table>

